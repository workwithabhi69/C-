#include <iostream>
using namespace std;

int main() {
    int num;
    cin >> num;

    bool isPronic = false;

    for (int i = 0; i * (i + 1) <= num; i++) {
        if (i * (i + 1) == num) {
            isPronic = true;
            break;
        }
    }

    if (isPronic)
        cout << "Pronic number";
    else
        cout << "Not a Pronic number";

    return 0;
}
