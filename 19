#include <iostream>
using namespace std;

int main() {
    int num, reversed = 0, temp;
    cin >> num;

    temp = num;

    while (temp > 0) {
        int digit = temp % 10;
        reversed = reversed * 10 + digit;
        temp /= 10;
    }

    if (num == reversed)
        cout << "Palindrome number";
    else
        cout << "Not a Palindrome number";

    return 0;
}
