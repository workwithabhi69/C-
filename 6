#include <iostream>
#include <string>
using namespace std;

string oneToNineteen[] = {"", "One", "Two", "Three", "Four", "Five", "Six",
                          "Seven", "Eight", "Nine", "Ten", "Eleven", "Twelve",
                          "Thirteen", "Fourteen", "Fifteen", "Sixteen",
                          "Seventeen", "Eighteen", "Nineteen"};

string tensMultiple[] = {"", "", "Twenty", "Thirty", "Forty", "Fifty",
                         "Sixty", "Seventy", "Eighty", "Ninety"};

string numberToWords(int n) {
    string result = "";

    if (n == 0)
        return "Zero";

    if (n / 1000 > 0) {
        result += oneToNineteen[n / 1000] + " Thousand ";
        n %= 1000;
    }

    if (n / 100 > 0) {
        result += oneToNineteen[n / 100] + " Hundred ";
        n %= 100;
    }

    if (n > 0) {
        if (n < 20)
            result += oneToNineteen[n];
        else {
            result += tensMultiple[n / 10];
            if (n % 10 > 0)
                result += " " + oneToNineteen[n % 10];
        }
    }

    return result;
}

int main() {
    int num;
    cin >> num;

    cout << numberToWords(num);

    return 0;
}
