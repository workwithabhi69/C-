#include <iostream>
using namespace std;

int main() {
    int a, b;
    cin >> a >> b;

    int gcd;
    int x = a, y = b;

    while (b != 0) {
        int temp = b;
        b = a % b;
        a = temp;
    }

    gcd = a;

    if (gcd == 1)
        cout << "Co-Prime numbers";
    else
        cout << "Not Co-Prime numbers";

    return 0;
}
