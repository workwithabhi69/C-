#include <iostream>
using namespace std;

int main() {
    int num, sum, temp;
    cin >> num;

    temp = num;

    while (sum > 9 || sum == 0) {
        sum = 0;
        while (temp > 0) {
            sum += temp % 10;
            temp /= 10;
        }
        temp = sum;
    }

    if (sum == 1)
        cout << "Magic number";
    else
        cout << "Not a Magic number";

    return 0;
}
