#include <iostream>
#include <cmath>
#include <string>
using namespace std;

int main() {
    int num;
    cin >> num;

    string s = to_string(num);
    int len = s.length();
    int sum = 0;

    for (int i = 0; i < len; i++) {
        int digit = s[i] - '0';
        sum += pow(digit, i + 1);
    }

    if (sum == num)
        cout << "Disarium number";
    else
        cout << "Not a Disarium number";

    return 0;
}
